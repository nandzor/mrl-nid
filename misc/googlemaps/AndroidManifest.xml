<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.android.googlemaps">
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-sdk android:minSdkVersion="8"></uses-sdk>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	
	<application android:name=".pointsManager" android:label="@string/app_name" android:icon="@drawable/androidmarker">
		<uses-library android:name="com.google.android.maps" />
		<activity android:name=".GoogleMaps" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".addGeopoints" android:label="@string/app_name"/>
		<activity android:name=".addDetails" android:label="@string/app_name"/>
		<activity android:name=".addSubtype" android:label="@string/app_name"/>
		<activity android:name=".gpsPoints" android:label="@string/app_name"/>
	</application>
</manifest>